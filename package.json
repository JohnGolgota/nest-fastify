{
  "name": "nestjs-fastify",
  "version": "1.0.0",
  "author": "",
  "keywords": [
    "typescript",
    "nestjs"
  ],
  "engines": {
    "node": ">=20.x"
  },
  "main": "dist/main.js",
  "scripts": {
    "build": "pnpm run build:clean && nest build --path tsconfig.prod.json",
    "start": "node dist/main.js",
    "start:dev": "tsnd --inspect=0.0.0.0:9229 --respawn src/main.ts",
    "test": "rimraf coverage .nyc_output && concurrently 'pnpm:test:unit' 'pnpm:test:e2e' && pnpm run coverage:merge",
    "test:unit": "jest tests/src --coverageDirectory coverage/unit",
    "test:e2e": "jest tests/e2e --coverageDirectory coverage/e2e",
    "coverage:merge": "ts-node scripts/merge-coverage.ts",
    "prepare": "husky install",
    "lint": "oxlint@latest",
    "lint:fix": "pnpm run lint -- --fix",
    "build:clean": "rimraf dist; exit 0"
  },
  "dependencies": {
    "@golevelup/ts-jest": "0.4.0",
    "@nestjs/cli": "10.3.0",
    "@nestjs/common": "10.3.0",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.3.0",
    "@nestjs/platform-fastify": "10.3.0",
    "reflect-metadata": "0.1.14",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.4",
    "@commitlint/config-conventional": "18.4.4",
    "@nestjs/schematics": "10.1.0",
    "@nestjs/testing": "10.3.0",
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.102",
    "@swc/jest": "0.2.29",
    "@types/fs-extra": "11.0.4",
    "@types/jest": "29.5.7",
    "@types/node": "20.11.0",
    "@types/supertest": "6.0.2",
    "concurrently": "8.2.2",
    "fs-extra": "11.2.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "lint-staged": "15.0.2",
    "nyc": "15.1.0",
    "rimraf": "5.0.5",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  }
}
